package com.gcettb;
import java.util.*;
public class Main {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        System.out.print("Enter the value of a and b:");
       int a= sc.nextInt();
       int b= sc.nextInt();
       int c=a;
       a=b;
       b=c;
        System.out.println("After swap the value of a: "+a+" b: "+b);

    }
}
