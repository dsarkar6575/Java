package com.gcettb;
import java.util.*;
public class Main {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        System.out.print("Enter the value of a and b:");
       int a= sc.nextInt();
       int b= sc.nextInt();
        System.out.println("The divisor: "+b);
        System.out.println("The  dividend: "+(a/b));
        System.out.println("The reminder is: "+(a%b));

    }
}
