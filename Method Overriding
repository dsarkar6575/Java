package com.gcettb;
class Animal{
    void speak(){
        System.out.println("The animal speaks.");
    }
}
class Dog extends Animal{
    @Override
    void speak()
    {
        System.out.println("The dog goes *bark*");
    }
}
public class Main {

    public static void main(String[] args) {
             //Method overriding = Declaring a method in sub class, which is already present in parent class. done so that a child class can give its own implementation
        Dog d=new Dog();
        Animal a=new Animal();
        d.speak();
        a.speak();

        }
    }
